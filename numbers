def english_to_arabic(text):
    numbers = {'0':'٠',   '1':'١',    '2': '٢',     '3':'٣' , '4':'٤' , '5':'٥', '6':'٦', '7':'٧', '8':'٨', '9':'٩'}
    text.replace('','')


    arabic = ""
    for x in text:
        arabic += numbers[x]

    return arabic 
    text.replace('','')
    text.split('')

text = input("enter number")

print (english_to_arabic(text))
